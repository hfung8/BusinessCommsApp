          <div class="tile eventTile is-child notification">
            <div class="content">
              <p class="title">Upcoming Events</p>
              <div class="content">
                <div class="modal" id="add-event-modal">
                  <div class="modal-background"></div>
                  <div class="modal-content">
                    <form method="POST" action='/add/event'>
                      <div class="field">
                        <label class="label">Event Name</label>
                        <p class="control">
                          <input class="input" type="text" placeholder="title" id='event-name' name="event_name">
                        </p>
                      </div>
                      <div class="field">
                        <label class="label">Start Date (mm-dd-yyyy)</label>
                        <p class="control">
                          <input class="input" type="date" placeholder="01-01-2017" id='start-date' name="start_date">
                        </p>
                      </div>
                      <div class="field">
                        <label class="label">End Date (mm-dd-yyyy)</label>
                        <p class="control">
                          <input class="input" type="date" placeholder="01-02-2017" id='end-date' name="end_date">
                        </p>
                      </div>
                      <div class="field">
                        <label class="label">Start Time (hh:mm am/pm)</label>
                        <p class="control">
                          <input class="input" type="time" placeholder="12:01 am" id='start-time' name="start_time">
                        </p>
                      </div>
                      <div class="field">
                        <label class="label">End (hh:mm am/pm)</label>
                        <p class="control">
                          <input class="input" type="time" placeholder="11:59 pm" id='end-time' name="end_time">
                        </p>
                      </div>
                      <div class="field">
                        <label class="label">Location</label>
                        <p class="control">
                          <input class="input" type="texttext" placeholder="New York, NY" id='location' name="event_location">
                        </p>
                      </div>
                      <div class="field">
                        <label class="label">Description</label>
                        <p class="control">
                          <textarea class="textarea"  placeholder="Description" id='description' name='event_description'></textarea>
                        </p>
                      </div>
                      <p class="control">
                        <button class="button is-primary" type='submit'>Submit</button>
                      </p>         
                    </form>
                  </div>
                  <button class="modal-close"></button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
